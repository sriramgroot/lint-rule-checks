{
	"extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
	"parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "constructor-super": 2,
        "for-direction": 2,
        "getter-return": 2,
        "no-async-promise-executor": 2,
        "no-case-declarations": 2,
        "no-class-assign": 2,
        "no-compare-neg-zero": 2,
        "no-cond-assign": 2,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-dupe-args": 2,
        "no-dupe-class-members": 2,
        "no-dupe-else-if": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-empty-pattern": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-func-assign": 2,
        "no-global-assign": 2,
        "no-import-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-loss-of-precision": 2,
        "no-misleading-character-class": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-nonoctal-decimal-escape": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-self-assign": 2,
        "no-setter-return": 2,
        "no-shadow-restricted-names": 2,
        "no-sparse-arrays": 2,
        "no-this-before-super": 2,
        "no-undef": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "no-unsafe-finally": 2,
        "no-unsafe-negation": 2,
        "no-unsafe-optional-chaining": 2,
        "no-unused-labels": 2,
        "no-unused-vars": 2,
        "no-useless-backreference": 2,
        "no-useless-catch": 2,
        "no-useless-escape": 2,
        "no-with": 2,
        "require-yield": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "array-bracket-newline": 2,
        "arrow-spacing": 2,
        "block-scoped-var": 2,
        "no-duplicate-imports": 2,
        "block-spacing": 2,
        "no-else-return": 2,
        "no-unmodified-loop-condition": 2,
        "brace-style": 2,
        "camelcase": 2,
        "no-empty-function": 2,
        "no-unreachable-loop": 2,
        "no-eq-null": 2,
        "comma-dangle": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "no-extra-bind": 2,
        "computed-property-spacing": 2,
        "no-use-before-define": 2,
        "curly": 2,
        "default-case": 2,
        "no-floating-decimal": 2,
        "default-case-last": 2,
        "eol-last": 2,
        "no-implied-eval": 2,
        "no-useless-return": 2,
        "no-var": 2,
        "func-call-spacing": 2,
        "no-whitespace-before-property": 2,
        "func-style": 2,
        "nonblock-statement-body-position": 2,
        "object-curly-newline": 2,
        "object-curly-spacing": 2,
        "arrow-body-style": 2,
        "arrow-parens": 2,
        "default-param-last": 2,
        "eqeqeq": 2,
        "id-length": 2,
        "implicit-arrow-linebreak": 2,
        "indent": ["error", "tab"],
        "init-declarations": 2,
        "jsx-quotes": ["error", "prefer-single"],
        "key-spacing": 2,
        "keyword-spacing": 2,
        "line-comment-position": 2,
        "lines-around-comment": 2,
        "multiline-comment-style": 2,
        "multiline-ternary": 2,
        "no-bitwise": 2,
        "no-confusing-arrow": 2,
        "no-console": 2,
        "no-magic-numbers": 2,
        "no-mixed-operators": 2,
        "no-multi-assign": 2,
        "no-multi-spaces": 2,
        "no-multiple-empty-lines": 2,
        "no-octal-escape": 2,
        "no-return-assign": 2,
        "no-return-await": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-template-curly-in-string": 2,
        "no-trailing-spaces": 2,
        "rest-spread-spacing": 2,
        "semi": 2,
        "semi-style": 2,
        "sort-imports": ["error", { "ignoreDeclarationSort": true }],
        "space-before-blocks": 2,
        "space-in-parens": ["error", "always"],
        "space-unary-ops": 2,
        "spaced-comment": ["error", "always"],
        "switch-colon-spacing": 2,
        "template-curly-spacing": ["error", "always"],
        "vars-on-top": 2
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },
	"plugins": ["react"]
}
